<!DOCTYPE html>
<html>
  <head>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= stylesheet_link_tag 'errors', media: 'all' %>
    <%= render 'layouts/head' %>
  </head>
  <body>
    <%= render 'layouts/navigation' %>
    <div>
      <%= render 'layouts/messages' %>
      <%= yield %>
    </div>
    <%= javascript_include_tag 'errors' %>
  </body>
</html>
