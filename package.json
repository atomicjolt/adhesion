{
  "name": "adhesion",
  "version": "0.0.1",
  "description": "Javascript for the Adhesion.",
  "engines": {
    "node": ">=10.17.0"
  },
  "scripts": {
    "test": "jest --config package.json",
    "test_update": "jest -u --config package.json",
    "test_debug": "node inspect jest --runInBand --config package.json || SUCCESS=false",
    "hot": "./bin/webpack-dev-server",
    "lint": "eslint client",
    "lint_fix": "eslint client --fix",
    "nuke": "rm -rf node_modules",
    "inspect_fuel": "\"${EDITOR:-vi}\" ./node_modules/atomic-fuel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atomicjolt/adhesion"
  },
  "keywords": [
    "React",
    "Rails",
    "Atomic Jolt",
    "LMS",
    "LTI",
    "OAuth"
  ],
  "jest": {
    "roots": [
      "<rootDir>/client"
    ],
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/client"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/testing/__mocks__/file_mock.js",
      "\\.(sass|scss|css|less)$": "<rootDir>/client/testing/__mocks__/style_mock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/client/testing/shim.js",
      "<rootDir>/client/testing/setup_env.js",
      "<rootDir>/client/testing/setup_tests.js"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "@apollo/react-testing": "^3.1.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.6.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.7.4",
    "@rails/webpacker": "^4.2.2",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-link-state": "^0.4.0",
    "atomic-canvas": "^1.8.0",
    "atomic-fuel": "^4.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-es2015": "^6.24.1",
    "core-js": "^3.2.1",
    "css-loader": "^2.1.1",
    "es6-promise": "^4.2.4",
    "file-loader": "^4.2.0",
    "file-saver": "^2.0.2",
    "fuse.js": "^3.0.5",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.5.0",
    "history": "^4.10.1",
    "lodash": "4.17.15",
    "mime": "^2.4.4",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.27",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-accessible-dropdown": "^1.0.2",
    "react-addons-css-transition-group": "^15.6.0",
    "react-apollo": "^3.1.2",
    "react-aria-menubutton": "^6.1.0",
    "react-copy-to-clipboard": "^5.0.0",
    "react-css-transition-replace": "^2.2.1",
    "react-datepicker": "^0.55.0",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dom": "^16.12.0",
    "react-dropzone-s3-uploader": "^1.0.0-rc.3",
    "react-modal": "^3.10.1",
    "react-modal1": "npm:react-modal@1.9.7",
    "react-paginate": "^5.2.4",
    "react-pdf": "^4.1.0",
    "react-proxy-loader": "^0.3.5",
    "react-radio-group": "^3.0.1",
    "react-redux": "^7.1.1",
    "react-redux5": "npm:react-redux@5.0.7",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router3": "npm:react-router@3.2.3",
    "react-select": "^3.0.4",
    "react-select-search": "^0.10.1",
    "recharts": "^1.8.5",
    "redux": "4.0.4",
    "redux3": "npm:redux@3.6.0",
    "regenerator-runtime": "^0.13.3",
    "resize-observer-polyfill": "^1.5.1",
    "sass-loader": "^8.0.0",
    "superagent": "^5.1.0",
    "uuid": "^3.3.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "eventsource-polyfill": "^0.9.6",
    "jest": "^24.9.0",
    "jest-transform-graphql": "^2.1.0",
    "nock": "^11.3.6",
    "react-test-renderer": "^16.10.2",
    "wait-for-expect": "^3.0.0",
    "webpack-dev-server": "^3.10.1"
  }
}
